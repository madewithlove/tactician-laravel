{
    "name": "madewithlove/tactician-laravel",
    "description": "Integrate tactician with Laravel 5",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Madewithlove",
            "email": "heroes@madewithlove.be"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/container": "^5.0|^5.1|^5.2|^5.3|^5.4|^5.5|^5.6",
        "illuminate/database": "^5.0|^5.1|^5.2|^5.3|^5.4|^5.5|^5.6",
        "illuminate/support": "^5.0|^5.1|^5.2|^5.3|^5.4|^5.5|^5.6",
        "league/tactician": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "mockery/mockery": "^0.9.4",
        "symfony/var-dumper": "^2.7",
        "friendsofphp/php-cs-fixer": "^2.15",
        "madewithlove/php-cs-fixer-config": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Madewithlove\\Tactician\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madewithlove\\Tactician\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Madewithlove\\Tactician\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    }
}
