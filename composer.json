{
    "name": "madewithlove/tactician-laravel",
    "description": "Integrate tactician with Laravel 5",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Madewithlove",
            "email": "heroes@madewithlove.be"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/container": "^5.0|^5.1|^5.2",
        "illuminate/database": "^5.0|^5.1|^5.2",
        "illuminate/support": "^5.0|^5.1|^5.2",
        "league/tactician": "^0.6.1"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "2.0.*@dev",
        "phpunit/phpunit": "^4.7",
        "mockery/mockery": "^0.9.4",
        "symfony/var-dumper": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Madewithlove\\Tactician\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madewithlove\\Tactician\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    }
}
